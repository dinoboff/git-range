{
  "name": "git-range",
  "version": "0.1.4",
  "description": "Parse git revision ranges for nodegit",
  "main": "src/index.js",
  "scripts": {
    "coverage": "nyc --reporter=text --reporter=lcov ava",
    "format": "npm run lint -- --fix",
    "lint": "eslint src/ test/",
    "report-coverage": "npm run coverage && codecov",
    "test": "ava -vv && npm run lint",
    "test-unit": "ava -vv",
    "test-watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dinoboff/git-range.git"
  },
  "keywords": [
    "nodegit",
    "git",
    "revision range"
  ],
  "author": {
    "name": "Damien Lebrun",
    "email": "dinoboff@hotmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dinoboff/git-range/issues"
  },
  "homepage": "https://github.com/dinoboff/git-range#readme",
  "devDependencies": {
    "ava": "^0.18.2",
    "child-process-promise": "^2.2.0",
    "codecov": "^1.0.1",
    "eslint": "^3.16.1",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-node": "^4.1.0",
    "nyc": "^10.1.2",
    "shelljs": "^0.7.6",
    "tempfile": "^1.1.1"
  },
  "files": [
    "example/",
    "src",
    "test/",
    "README.md",
    "LICENSE.md"
  ],
  "directories": {
    "lib": "src",
    "example": "example"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "lodash.flatten": "^4.4.0"
  },
  "peerDependencies": {
    "nodegit": "^0.17.0"
  }
}
